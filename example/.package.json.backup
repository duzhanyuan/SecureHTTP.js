{
    "name": "securehttp",
    "version": "0.2.0",
    "description": "Make HTTP transmissions more secure via RSA+AES, encrypted communication for nodejs or browser.",
    "main": "src/securehttp.js",
    "browserify": {
        "transform": [
            [
                "babelify",
                {
                    "presets": [
                        "es2015"
                    ]
                }
            ]
        ]
    },
    "dependencies": {
        "crypto-js": "^3.1.9-1",
        "node-jsencrypt": "https://github.com/ArnaudValensi/node-jsencrypt"
    },
    "devDependencies": {
        "babel-core": "^6.26.0",
        "babel-preset-es2015": "^6.24.1",
        "babelify": "^8.0.0",
        "browserify": "^16.5.0",
        "uglify-js": "^3.6.0"
    },
    "scripts": {
        "build": "browserify -r ./securehttp.js -s securehttp -o ./dist/securehttp.js",
        "dist": "browserify -r ./securehttp.js -s securehttp | uglifyjs > ./dist/securehttp.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/staugur/SecureHTTP.js"
    },
    "keywords": [
        "AES",
        "RSA",
        "Crypto-JS"
    ],
    "author": "staugur",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/staugur/SecureHTTP.js/issues"
    },
    "homepage": "https://github.com/staugur/SecureHTTP.js"
}
